{
  "name": "miniorg",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "BUILD_TARGET=dev next dev",
    "build": "prisma generate && next build",
    "build:local": "BUILD_TARGET=dev npm run build",
    "build:cloudflare": "BUILD_TARGET=cloudflare prisma generate && BUILD_TARGET=cloudflare opennextjs-cloudflare build",
    "deploy:cloudflare": "npm run db:migrate:d1:apply:remote && npm run build:cloudflare && opennextjs-cloudflare deploy",
    "start": "next start",
    "lint": "next lint",
    "preview": "BUILD_TARGET=cloudflare opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "tauri": "tauri",
    "tauri:dev": "BUILD_TARGET=tauri tauri dev",
    "tauri:build": "cp .env .env.backup && cp .env.tauri .env && BUILD_TARGET=tauri tauri build; mv .env.backup .env",
    "build:tauri": "BUILD_TARGET=tauri next build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --url=\"file:./dev.db\"",
    "db:push:accept-data-loss": "prisma db push --url=\"file:./prisma/dev.db\" --accept-data-loss",
    "db:migrate:deploy": "DATABASE_URL=\"file:./dev.db\" prisma migrate deploy --config=./prisma/prisma.config.ts",
    "db:studio": "prisma studio --url=\"file:./dev.db\"",
    "db:reset": "rm -f ./dev.db ./dev.db-journal && npm run db:push",
    "db:migrate:d1:create": "wrangler d1 migrations create DB",
    "db:migrate:d1:apply": "wrangler d1 migrations apply DB",
    "db:migrate:d1:apply:local": "wrangler d1 migrations apply DB --local",
    "db:migrate:d1:apply:remote": "wrangler d1 migrations apply DB --remote",
    "db:types": "wrangler types --env-interface CloudflareEnv"
  },
  "dependencies": {
    "@atlaskit/pragmatic-drag-and-drop": "^1.1.0",
    "@atlaskit/pragmatic-drag-and-drop-react-drop-indicator": "^1.0.0",
    "@auth/prisma-adapter": "^1.0.0",
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/interaction": "^6.1.20",
    "@fullcalendar/react": "^6.1.20",
    "@fullcalendar/timegrid": "^6.1.20",
    "@libsql/client": "^0.17.0",
    "@opennextjs/cloudflare": "^1.14.9",
    "@prisma/adapter-d1": "^7.0.0",
    "@prisma/adapter-libsql": "^7.2.0",
    "@prisma/client": "^7.0.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tauri-apps/plugin-deep-link": "^2.4.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.309.0",
    "next": "^14.1.0",
    "next-auth": "^5.0.0-beta.9",
    "novel": "^0.1.22",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-resizable-panels": "^4.4.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/cli": "^2.0.0",
    "@tauri-apps/plugin-global-shortcut": "^2.0.0",
    "@tauri-apps/plugin-notification": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "postcss": "^8.4.32",
    "prisma": "^7.0.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.0"
  }
}
